{
  "name": "scootify-player",
  "version": "1.0.0",
  "description": "audio player for scootify playlist",
  "author": "Eliana Sur",
  "main": "app.js",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --watch",
    "db-create": "mysql -uroot <./server/scripts/schema.sql",
    "db-seed": "node ./server/scripts/seed_db.js",
    "server-dev": "nodemon server/app.js",
    "test": "mocha server/spec/server-spec.js",
    "lint": "eslint ./"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.15.0",
    "jquery": "^3.1.1",
    "morgan": "^1.4.1",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.9",
    "request": "^2.88.0",
    "sequelize": "^3.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "chai": "^3.5.0",
    "css-loader": "^2.1.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "jest": "^24.1.0",
    "mocha": "^5.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "sql-template-strings": "^2.2.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
